@model Huertos_Autosustentables.Models.DetalleUsersCultivo

@{
    ViewData["Title"] = "Delete";
}
<div class="m-5">
    <h3>Estas seguro de borra estos datos extra </h3>
    <div>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.MetrosCuadradosDetalle)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MetrosCuadradosDetalle)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.PrecioSemillasDetalle)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.PrecioSemillasDetalle)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.LugarCultivoDetalle)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.LugarCultivoDetalle)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.IdCultivo)
            </dt>
            <dd class="col-sm-10">
                <select asp-for="IdCultivo" disabled="disabled" style="background-color: transparent; border:none; -webkit-appearance: none; color:black" asp-items="ViewBag.IdCultivo"></select>
            </dd>
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="IdDetalle" />
            <input type="submit" value="Borrar" class="btn btn-danger" /> |
            <a class="btn btn-info" asp-action="Index">Regresar a la lista</a>
        </form>
    </div>
</div>
